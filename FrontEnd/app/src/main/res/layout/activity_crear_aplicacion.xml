<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".CrearAplicacionActivity" tools:layout_editor_absoluteY="81dp">
    <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/txt_id_ap"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView14" app:layout_constraintEnd_toStartOf="@+id/btn_foto"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:layout_marginEnd="55dp"/>
    <TextView
            android:text="Id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView14"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="@+id/txt_id_ap"
            android:layout_marginTop="15dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:id="@+id/txt_url_ap"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="15dp"
            app:layout_constraintTop_toBottomOf="@+id/textView13" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:layout_marginEnd="32dp" android:inputType="textWebEmailAddress"/>
    <TextView
            android:text="@string/url_descarga"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView13"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="@+id/txt_peso_ap" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_peso_ap"/>
    <TextView
            android:text="@string/peso_gigas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="@+id/txt_lanzamiento_ap" android:layout_marginTop="7dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_lanzamiento_ap"/>
    <TextView
            android:text="@string/costo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="7dp"
            android:layout_marginStart="47dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_lanzamiento_ap"
            app:layout_constraintStart_toEndOf="@+id/txt_peso_ap"/>
    <TextView
            android:text="@string/nombre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="@+id/txt_id_ap" android:layout_marginTop="9dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_id_ap"/>
    <TextView
            android:text="@string/version_api"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:id="@+id/textView9"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toTopOf="@+id/txt_nombres_ap"
            app:layout_constraintStart_toStartOf="@+id/txt_nombres_ap" android:layout_marginTop="54dp"
            app:layout_constraintBottom_toBottomOf="@+id/txt_versiones_ap" android:layout_marginBottom="54dp"/>
    <TextView
            android:text="@string/fecha_lanzamiento"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:id="@+id/textView10"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toTopOf="@+id/txt_versiones_ap"
            app:layout_constraintStart_toStartOf="@+id/txt_versiones_ap" android:layout_marginTop="54dp"
            app:layout_constraintBottom_toBottomOf="@+id/txt_lanzamiento_ap" android:layout_marginBottom="54dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txt_nombres_ap"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="9dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:layout_marginEnd="32dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/txt_versiones_ap"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="36dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_nombres_ap" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:layout_marginEnd="32dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:ems="10"
            android:id="@+id/txt_lanzamiento_ap"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="36dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_versiones_ap" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:layout_marginEnd="32dp"/>
    <EditText
            android:layout_width="154dp"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/txt_peso_ap"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="@+id/textView11" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/textView11"/>

    <Button
            android:text="@string/btn_guardar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_guardar_ap"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="80dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_url_ap" app:layout_constraintVertical_bias="0.496"/>
    <Button
            android:text="@string/btn_cancelar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_cancelar_ap"
            tools:ignore="MissingConstraints"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_url_ap"
            app:layout_constraintStart_toEndOf="@+id/btn_guardar_ap" android:layout_marginStart="8dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/txt_costo_ap"
            app:layout_constraintStart_toStartOf="@+id/textView12" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/textView12"
            app:layout_constraintEnd_toEndOf="@+id/txt_lanzamiento_ap"/>
    <Button
            android:text="Tomar Foto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_foto" android:layout_marginTop="39dp"
            app:layout_constraintStart_toEndOf="@+id/txt_id_ap" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="32dp"
    />
</android.support.constraint.ConstraintLayout>