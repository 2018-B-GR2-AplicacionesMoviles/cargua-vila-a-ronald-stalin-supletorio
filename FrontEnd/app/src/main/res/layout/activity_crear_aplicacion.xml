<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".CrearAplicacionActivity" tools:layout_editor_absoluteY="81dp">
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/txt_id_ap"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView14" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:layout_marginEnd="32dp"/>
    <TextView
            android:text="Id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView14"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="@+id/txt_id_ap" android:layout_marginTop="15dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/txt_url_ap"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView13" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:layout_marginEnd="32dp"/>
    <TextView
            android:text="@string/url_descarga"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView13"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="@+id/txt_url_ap"
            android:layout_marginTop="10dp" app:layout_constraintTop_toBottomOf="@+id/txt_costo_ap"/>
    <TextView
            android:text="@string/peso_gigas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="@+id/txt_peso_gigas_ap"
            android:layout_marginTop="10dp" app:layout_constraintTop_toBottomOf="@+id/txt_fecha_lanzamiento_ap"/>
    <TextView
            android:text="@string/costo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="@+id/txt_costo_ap"
            android:layout_marginTop="10dp" app:layout_constraintTop_toBottomOf="@+id/txt_peso_gigas_ap"/>
    <TextView
            android:text="@string/nombre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3"
            tools:ignore="MissingConstraints"
            app:layout_constraintStart_toStartOf="@+id/txt_id_ap"
            android:layout_marginTop="9dp" app:layout_constraintTop_toBottomOf="@+id/txt_id_ap"/>
    <TextView
            android:text="@string/version_api"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:id="@+id/textView9"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toTopOf="@+id/txt_nombre_ap"
            app:layout_constraintStart_toStartOf="@+id/txt_nombre_ap" android:layout_marginTop="54dp"
            app:layout_constraintBottom_toBottomOf="@+id/txt_version_ap" android:layout_marginBottom="54dp"/>
    <TextView
            android:text="@string/fecha_lanzamiento"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:id="@+id/textView10"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toTopOf="@+id/txt_version_ap"
            app:layout_constraintStart_toStartOf="@+id/txt_version_ap" android:layout_marginTop="54dp"
            app:layout_constraintBottom_toBottomOf="@+id/txt_fecha_lanzamiento_ap" android:layout_marginBottom="54dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txt_nombre_ap"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="9dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:layout_marginEnd="32dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/txt_version_ap"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="36dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_nombre_ap" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:layout_marginEnd="32dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="date"
            android:ems="10"
            android:id="@+id/txt_fecha_lanzamiento_ap"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="36dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_version_ap" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:layout_marginEnd="32dp"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/txt_peso_gigas_ap"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView11" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:layout_marginEnd="32dp"/>

    <Button
            android:text="@string/btn_guardar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_guardar_so"
            tools:ignore="MissingConstraints"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_url_ap" android:layout_marginStart="80dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.23"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/btn_cancelar_so"
            app:layout_constraintHorizontal_bias="0.234"/>
    <Button
            android:text="@string/btn_cancelar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btn_cancelar_so"
            tools:ignore="MissingConstraints"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="92dp"
            android:layout_marginBottom="48dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/txt_url_ap" app:layout_constraintVertical_bias="1.0"/>
    <EditText
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:inputType="numberDecimal"
            android:ems="10"
            android:id="@+id/txt_costo_ap"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView12" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp" android:layout_marginEnd="32dp"/>
</android.support.constraint.ConstraintLayout>