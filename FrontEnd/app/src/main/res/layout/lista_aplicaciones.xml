            android:textColor="@color/alert_default_text_color"
            android:layout_margin="5dp" tools:layout_editor_absoluteX="14dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent" tools:ignore="MissingConstraints"/>
    <TextView
            android:id="@+id/version_lista_so"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="VersiÃ³n"
            android:textSize="14sp"
            android:layout_marginTop="8dp"
            android:textColor="@color/alert_default_text_color"
            android:layout_margin="5dp"
